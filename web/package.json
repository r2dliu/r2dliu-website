{
  "name": "r2dliu-website",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix build",
    "dev": "run-p dev:*",
    "dev:css": "sass --watch app/:app/",
    "dev:remix": "remix dev --manual -c \"arc sandbox -e testing\"",
    "format": "prettier --write .",
    "format:repo": "npm run format && npm run lint -- --fix",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "start": "cross-env NODE_ENV=production arc sandbox"
  },
  "prettier": {},
  "eslintIgnore": [
    "/node_modules",
    "/server/index.js",
    "/public/build"
  ],
  "dependencies": {
    "@architect/architect": "^10.14.2",
    "@architect/functions": "^7.0.0",
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@mui/icons-material": "^5.11.9",
    "@mui/material": "^5.11.10",
    "@remix-run/architect": "*",
    "@remix-run/css-bundle": "*",
    "@remix-run/node": "*",
    "@remix-run/react": "*",
    "classnames": "^2.3.2",
    "framer-motion": "^9.0.7",
    "isbot": "^3.6.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-lazy-load-image-component": "^1.6.0",
    "react-markdown": "^8.0.6",
    "react-player": "^2.12.0",
    "react-toc": "^3.1.0",
    "rehype-raw": "^6.1.1",
    "remix-island": "^0.1.2",
    "tocbot": "^4.21.0"
  },
  "devDependencies": {
    "@remix-run/dev": "*",
    "@remix-run/eslint-config": "*",
    "@types/architect__functions": "^3.13.7",
    "@types/eslint": "^8.4.10",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.10",
    "@types/react-lazy-load-image-component": "^1.5.2",
    "cross-env": "^7.0.3",
    "esbuild": "^0.19.5",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "happy-dom": "^8.1.4",
    "msw": "^1.3.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.3",
    "sass": "^1.58.3",
    "start-server-and-test": "^1.15.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=18"
  }
